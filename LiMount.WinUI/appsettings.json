{
  "LiMount": {
    "ScriptExecution": {
      "TempFilePollingTimeoutSeconds": 5,
      "PollingIntervalMs": 100
    },
    "MountOperations": {
      "UncAccessibilityRetries": 5,
      "UncAccessibilityDelayMs": 500,
      "ReconcileUncAccessibilityTimeoutMs": 2000,
      "UncExistenceCheckTimeoutMs": 2000,
      "UncPathCheckTimeoutMs": 2000,
      "SubstCommandTimeoutMs": 5000,
      "WslCommandTimeoutMs": 5000,
      "RollbackTimeoutMs": 5000
    },
    "History": {
      "MaxEntries": 100,
      "FilePath": null,
      "StateFilePath": null
    },
    "Initialization": {
      "MaxRetries": 2,
      "BaseDelayMs": 1000,
      "MaxDelayMs": 10000,
      "AutoReconcileMounts": true,
      "AutoDetectSystemMounts": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  }
}
